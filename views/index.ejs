<html>
<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

 <!-- jQuery library -->
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

 <!-- Latest compiled JavaScript -->
 <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
<div class="container">
 <br>
 <div class="jumbotron">
 <h1 class="display-4">Send Message</h1>
 <br>

 <form action="/" method="post">
 <input id = "name" class="form-control" placeholder="Name" name="newName"  autocomplete="off">
 <br>
 <textarea id = "message" class="form-control" placeholder="Your Message Here" name="newMessage"  autocomplete="off">
</textarea>
 <br>
 <button type="submit" name="button" id="send" class="btn btn-success">Send</button>
</form>
 </div>

 <% for (item in newListItem){ %>
 <div id="messages">
   <h2> <%= newListItem[item].name %> </h2>
   <p> <%= newListItem[item].message %> </p>
</div>
<% } %>
</div>
<script>


</script>
</body>
</html>
